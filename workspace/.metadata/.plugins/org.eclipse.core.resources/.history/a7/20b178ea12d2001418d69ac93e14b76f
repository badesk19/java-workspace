package com.bank;		//package name

public class BankAccount {		//class name
	
	String accountHolderName;
	String address;
	double rateOfInterest;
	double balance;
	int durationInYear;
	
	public BankAccount() {		//default constructor
	}
	
	
	
	public BankAccount(String accountHolderName, String address,
			double rateOfInterest, double balance, int durationInYear) {
		this.accountHolderName = accountHolderName;
		this.address = address;
		this.rateOfInterest = rateOfInterest;
		this.balance = balance;
		this.durationInYear = durationInYear;
	}



	public double calculateTotalAmount(int amount)
	{
		 double totalAmount=0;
		return totalAmount; 
		
	}
	
	
}

